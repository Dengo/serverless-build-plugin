{
  "name": "serverless-build-plugin",
  "version": "0.6.3",
  "main": "./dist/index",
  "description": "A Node.js focused build optimizer plugin for serverless",
  "scripts": {
    "build": "babel $BABEL_OPTIONS --source-maps inline ./src --out-dir ./dist",
    "dev": "BABEL_OPTIONS=--watch npm run build",
    "prepublish": "npm run build && npm run test",
    "test": "jest"
  },
  "dependencies": {
    "babel-core": "~6.18.0",
    "babel-preset-babili": "~0.0.9",
    "bluebird": "~3.4.0",
    "chalk": "^1.1.3",
    "findit": "^2.0.0",
    "fs-extra": "~0.26.0",
    "graceful-fs": "^4.1.11",
    "is-stream": "~1.1.0",
    "js-yaml": "~3.6.1",
    "lutils": "^1.1.0",
    "minimatch": "~3.0.3",
    "mkdirp": "~0.5.1",
    "resolve-pkg": "~0.2.0",
    "source-map-support": "~0.4.6",
    "uglify-js": "~2.7.0",
    "yauzl": "~2.6.0",
    "yazl": "~2.4.1"
  },
  "devDependencies": {
    "babel-cli": "~6.18.0",
    "babel-polyfill": "~6.16.0",
    "babel-eslint": "~6.1.2",
    "babel-jest": "^17.0.2",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-async-to-module-method": "~6.16.0",
    "babel-preset-es2015-node4": "~2.1.0",
    "babel-preset-stage-1": "~6.16.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^17.0.3",
    "nyc": "~6.2.1",
    "webpack": "2.1.0-beta.24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nfour/serverless-build-plugin"
  }
}
